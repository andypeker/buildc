# Project information
TOPDIR = @topdir@

# Platform information
OS = @os@
CPU = @cpu@
CMODE = @cmode@

# Version information
VERSION = @version@

# Compiler options
DEFS = -D_REENTRANT -D_POSIX_PTHREAD_SEMANTICS -DVERSION=\"${VERSION}\"
FPIC  = -fPIC
FDEBUG  = -g
FWALL = -Wall
CUSTOM_DEFS = @custom_defs@
CC = @cc@
CFLAGS = $(FDEBUG) $(FWALL) $(FPIC) $(DEFS) $(CUSTOM_DEFS) $(INCLUDES)

# Library infomation
@lib_roots@

LIB_INCLUDES = @lib_includes@

LIBS_DEPEND = @libs_depend@

CUSTOM_LIBS = @custom_libs@

SYSTEM_LIBS = @system_libs@

# Headers
DEFAULT_INCLUDES = @default_includes@

CUSTOM_INCLUDES = @custom_includes@

INCLUDES = -I $(TOPDIR)/include $(LIB_INCLUDES) $(CUSTOM_INCLUDES) $(DEFAULT_INCLUDES)

# Libraries
DEFAULT_LIBS = @default_libs@

LIBS = $(CUSTOM_LIBS) $(LIBS_DEPEND) $(DEFAULT_LIBS) $(SYSTEM_LIBS)

STATIC_LIBS = @static_libs@

DYNAMIC_LIBS = @dynamic_libs@

# Other definitions
@custom_vars@

# Rules
OBJ = $(SRC:.c=.o)
HFILE = $(SRC:.c=.h)
TARGET = $(SRC:.c=.a)
DSO = $(SRC:.c=.so)

${OBJ}: ${SRC}
	${CC} ${CFLAGS} -c ${SRC}

${DSO}: ${SRC}
	${CC} ${CFLAGS} ${SHARED} $@ -c ${SRC}
